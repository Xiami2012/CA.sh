[ default ]
#RANDFILE    =
oid_file    = ext.oid
#oid_section =

[ req ]
#input_password  =
#output_password =
#default_bits    = 512
#default_keyfile =
#encrypt_key     = no
default_md      = sha512
string_mask     = utf8only
#req_extensions  = v3ext
#x509_extensions = v3ext
#prompt          = no
utf8            = yes
#attributes      =
distinguished_name  = req_dn

[ req_dn ]
countryName             = Country Name (2 letter code)
countryName_default     = CN
countryName_min         = 2
countryName_max         = 2
stateOrProvinceName     = State or Province Name (eg, Hubei)
localityName            = Locality Name (eg, Wuhan)
organizationName        = Organization Name
organizationUnitName    = Organization Unit Name
commonName              = Common Name
emailAddress            = Email Address
